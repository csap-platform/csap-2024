	"service-templates": {

		"fio": {
			"server": "csap-api",
			"description": "run file io testing using fio",
			"url": "$$csap-agent-url/app-browser?defaultService=$$service-name#agent-tab__comma__file-browser",
			"appDirectory": "/",
			"propDirectory": "/etc/mysql",
			"scheduledJobs": {
				"scripts": [
					{
						"description": "fio disk tests",
						"background": true,
						"frequency": "daily",
						"hours": [
							"19",
							"20"
						],
						"script": "$$service-resources/common/fio-tester.sh $$service-name",
						"parameters": [
							"summary,__fio disk,-- invokes fio tests readme for more details  -- refer to <a href='https://fio.readthedocs.io/en/latest/fio_doc.html#command-line-options'>manual</a> ",
							"timeToRunInSeconds,120,duration of test",
							"blockSize,4k,block size used during tests",
							"testSize,1g,each iteration repeated until timeToRunInSeconds",
							"threadIterations,1 2 4,thread counts space delimited to be tested",
							"readPercent,70,percentage of operations that will be reads",
							"ioDepth,16,fio depth. 1 is fully synchronous operations"
						]
					}
				]
			},
			"performance-local": {
				"config": {
					"httpCollectionUrl": "file:/httpCollect/fio.sh",
					"patternMatch": "byWordIndex",
					"lines-to-read": 1,
					"word-count-filter": 12
				}
			},

			"performance": {
				"config": {
					"httpCollectionUrl": "file:/httpCollect/fio.sh",
					"patternMatch": "byWordIndex",
					"lines-to-read": 1,
					"word-count-filter": 12
				},
				"threads": {
					"attribute": 1,
					"title": "cpu threads used"
				},
				"totalTimeSeconds": {
					"attribute": 11,
					"title": "Run Duration Seconds"
				},
				"readBwPerSecond": {
					"attribute": 4,
					"title": "Read BW Mb Per Second"
				},
				"readIoOps": {
					"attribute": 3,
					"decimals": 2,
					"title": "Read IO Operations Per Second"
				},
				"readMeanNs": {
					"attribute": 5,
					"title": "Read Latency (ns)"
				},
				"rwriteMeanNs": {
					"attribute": 6,
					"title": "Write Latency (ns)"
				},
				"writeBwPerSecond": {
					"attribute": 8,
					"title": "Write BW Mb Per Second"
				},
				"writeIoOps": {
					"attribute": 7,
					"decimals": 2,
					"title": "Write IO Operations Per Second"
				},
				"diskReadIos": {
					"attribute": 9,
					"title": "Disk Read IO Operations Per Second"
				},
				"diskWriteIos": {
					"attribute": 10,
					"title": "Disk Write IO Operations Per Second"
				},
				"blockSizeInKb": {
					"attribute": 2,
					"title": "Block Size Tested (kb)"
				},
				"diskPerIterationGb": {
					"attribute": 12,
					"title": "Disk Per Iteration (gb)"
				}
			},
			"deploymentNotes": "start stop will do a short run, use jobs to trigger start and stop"
		},

		"sysbench": {
			"definitionSource": "default-project.json",
			"server": "docker",
			"description": "added by peter.nightingale",
			"url": "http://$$service-fqdn-host:$$service-primary-port",
			"appDirectory": "/",
			"propDirectory": "/etc/mysql",
			"environmentVariables": {
				"configuration-maps": [
					"mysql-settings"
				],
				"PMA_HOST": "$$db-host"
			},
			"docker": {
				"image": "docker-public-artifactory.yourcompany.com/severalnines/sysbench:latest",
				"autoStart": false,
				"network": {
					"name": "$$db-host-network",
					"note": "name can be bridge, host, or custom network name",
					"createPersistent": {
						"enabled": true,
						"driver": "bridge"
					}
				}
			},
			"performance": {
				"config": {
					"httpCollectionUrl": "file:/httpCollect/sysbench-mysql.sh",
					"patternMatch": "byWordIndex",
					"lines-to-read": 1,
					"line-start-filter": "["
				},
				"threads": {
					"attribute": "5",
					"title": "threads"
				},
				"tps": {
					"attribute": "7",
					"title": "tps"
				},
				"qps": {
					"attribute": "9",
					"title": "qps"
				},
				"latencyMs": {
					"attribute": "14",
					"title": "latencyMs"
				},
				"errorsPerSecond": {
					"attribute": "16",
					"title": "errorsPerSecond"
				}
			}
		},
		
		"multijvm1": {
			"definitionSource": "default-project.json",
			"server": "csap-api",
			"description": "placeholder for custom processes",
			"jmxPort": 12345,
			"metaData": "collectTomcat",
			"docUrl": "https://confluence.yourcompany.com/display/oms/OMS+New+Hire+Resources",
			"url": "https://confluence.yourcompany.com/display/oms/OMS+New+Hire+Resources",
			"appDirectory": "$$application-folder/ots",
			"propDirectory": "$$application-folder/ots/tomcat/conf",
			"processFilter": ".*tag=ots.*",
			"alerts": {
				"max_topCpu": 1000,
				"max_diskUtil": "20g",
				"max_diskWriteKb": "4000",
				"max_threadCount": 700,
				"max_rssMemory": "5g",
				"max_socketCount": 200
			},
			"isMessaging": "true",
			"deployTimeoutMinutes": 20,
			"maven": {
				"dependency": "oms-csap-api.sh"
			},
			"logDirectory": "$$application-folder/ots/logs",
			"defaultLogToShow": "ots.log",
			"scheduledJobs": {
				"scripts": [
					{
						"description": "wait for ots start message",
						"frequency": "event-post-start",
						"script": "wait_for_log 'Startup for' '$$application-folder/ots/logs/ots.log' 100 10"
					},
					{
						"description": "simple listing",
						"frequency": "onDemand",
						"background": false,
						"script": "su - $$application-user -c \"ls $$application-folder/ots\"",
						"parameters": [
							"userid,not implemented yet,peter should implement if asked"
						]
					}
				]
			},
			"performance-http": {
				"config": {
					"httpCollectionUrl": "classpath:/httpCollect/demo-prometheus.sh",
					"patternMatch": " ([^\n]*)",
					"jmxDualMode": true
				},
				"instanceDecompressionCount": {
					"attribute": "my_prom_app_id_backconvdatametrics_instances_decompressiontime_count.*",
					"title": "my_prom_app_id_backconvdatametrics_instances_decompressiontime_count"
				}
			},
			"performance": {
				"config": {
					"httpCollectionUrl": "classpath:/httpCollect/demo-prometheus.sh",
					"patternMatch": " ([^\n]*)",
					"jmxDualMode": true,
					"about": "after ssh tunnel - desktop trys a test vm, force the tunnel using override",
					"jmxHostOverride": "my-core-host2.some.host.pattern"
				},
				"HostCpu": {
					"mbean": "java.lang:type=OperatingSystem",
					"attribute": "SystemCpuLoad",
					"max": 40,
					"title": "Host CPU"
				},
				"ProcessCpu": {
					"mbean": "java.lang:type=OperatingSystem",
					"attribute": "ProcessCpuLoad",
					"title": "JVM Cpu"
				},
				"Free Memory": {
					"mbean": "java.lang:type=OperatingSystem",
					"attribute": "FreePhysicalMemorySize",
					"title": "FreePhysicalMemorySize(Mb)",
					"divideBy": 1048576,
					"decimals": 0,
					"errors": true
				},
				"classesLoaded": {
					"title": "JVM Classes Loaded",
					"mbean": "java.lang:type=ClassLoading",
					"attribute": "LoadedClassCount"
				},
				"zingNewGcPercentRunning": {
					"title": "Zing New GC Percentage of Time Collector is Running",
					"info": "test size limit",
					"mbean": "com.azul.zing:type=GarbageCollector,name=GPGC New",
					"attribute": "PercentageOfTimeCollectorIsRunning",
					"decimals": 2,
					"max": 10
				},
				"zingOldGcPercentRunning": {
					"title": "Zing Old GC 2 Percentage of Time Collector is Running",
					"mbean": "com.azul.zing:type=GarbageCollector,name=GPGC Old",
					"attribute": "PercentageOfTimeCollectorIsRunning",
					"decimals": 2,
					"max": 10
				},
				"zingNewGcFound": {
					"title": "Zing New GC Found (Mb)",
					"mbean": "com.azul.zing:type=GarbageCollector,name=GPGC New",
					"attribute": "LastGCDetails,,garbageFound",
					"divideBy": 1048576,
					"max": 10
				},
				"zingNewGcCollected": {
					"title": "Zing New GC Collected (Mb)",
					"mbean": "com.azul.zing:type=GarbageCollector,name=GPGC New",
					"attribute": "LastGCDetails,,garbageCollected",
					"divideBy": 1048576,
					"max": 10
				},
				
				"zingNewGcAllocRate": {
					"title": "Zing New GC Allocation (Mb)/s",
					"mbean": "com.azul.zing:type=GarbageCollector,name=GPGC New",
					"attribute": "LastGCDetails,,allocationRateBetweenEndOfPreviousAndStart",
					"decimals": 1,
					"max": 700
				},
				
				"zingNewGcContingency": {
					"title": "Zing New GC Contingency Memory Used",
					"mbean": "com.azul.zing:type=GarbageCollector,name=GPGC New",
					"attribute": "LastGCDetails,,hasContingencyMemoryBeenUsedSinceEndOfPreviousCollection",
					"max": 0
				},
				
				"zingNewGcPausePrevent": {
					"title": "Zing New GC PausePrevent Memory Used",
					"mbean": "com.azul.zing:type=GarbageCollector,name=GPGC New",
					"attribute": "LastGCDetails,,hasPausePreventionMemoryBeenUsedSinceEndOfPreviousCollection",
					"max": 0
				},
				
				"zingNewGcThreadDelay": {
					"title": "Zing New GC Thread Delay (s)",
					"mbean": "com.azul.zing:type=GarbageCollector,name=GPGC New",
					"attribute": "LastGCDetails,,maximumApplicationThreadDelay",
					"decimals": 1,
					"max": 0
				},
				
				"zingNewGcPauseTime": {
					"title": "Zing Pause Time Total",
					"mbean": "com.azul.zing:type=GarbageCollector,name=GPGC New",
					"attribute": "CollectorCumulativePauseTimeSec",
					"decimals": 2,
					"max": 10
				},
				"zingNewPauseTimeDelta": {
					"title": "Zing Pause Time Change",
					"mbean": "com.azul.zing:type=GarbageCollector,name=GPGC New",
					"attribute": "CollectorCumulativePauseTimeSec",
					"decimals": 2,
					"delta": "delta",
					"max": 10
				},
				
				"zingOldGcFound": {
					"title": "Zing Old GC Found (Mb)",
					"mbean": "com.azul.zing:type=GarbageCollector,name=GPGC Old",
					"attribute": "LastGCDetails,,garbageFound",
					"divideBy": 1048576,
					"max": 10
				},
				"zingOldGcCollected": {
					"title": "Zing Old GC Collected (Mb)",
					"mbean": "com.azul.zing:type=GarbageCollector,name=GPGC Old",
					"attribute": "LastGCDetails,,garbageCollected",
					"divideBy": 1048576,
					"max": 10
				},
				
				"zingOldGcAllocRate": {
					"title": "Zing Old GC Allocation (Mb)/s",
					"mbean": "com.azul.zing:type=GarbageCollector,name=GPGC Old",
					"attribute": "LastGCDetails,,allocationRateBetweenEndOfPreviousAndStart",
					"decimals": 1,
					"max": 700
				},
				
				"zingOldGcContingency": {
					"title": "Zing Old GC Contingency Memory Used",
					"mbean": "com.azul.zing:type=GarbageCollector,name=GPGC Old",
					"attribute": "LastGCDetails,,hasContingencyMemoryBeenUsedSinceEndOfPreviousCollection",
					"max": 0
				},
				
				"zingOldGcPausePrevent": {
					"title": "Zing Old GC PausePrevent Memory Used",
					"mbean": "com.azul.zing:type=GarbageCollector,name=GPGC Old",
					"attribute": "LastGCDetails,,hasPausePreventionMemoryBeenUsedSinceEndOfPreviousCollection",
					"max": 0
				},
				
				"zingOldGcThreadDelay": {
					"title": "Zing Old GC Thread Delay (s)",
					"mbean": "com.azul.zing:type=GarbageCollector,name=GPGC Old",
					"attribute": "LastGCDetails,,maximumApplicationThreadDelay",
					"decimals": 1,
					"max": 0
				},
				
				"zingOldGcPauseTime": {
					"title": "Zing Pause Time Total",
					"mbean": "com.azul.zing:type=GarbageCollector,name=GPGC Old",
					"attribute": "CollectorCumulativePauseTimeSec",
					"decimals": 2,
					"max": 10
				},
				
				"zingOldPauseTimeDelta": {
					"title": "Zing Pause Time Change",
					"mbean": "com.azul.zing:type=GarbageCollector,name=GPGC Old",
					"attribute": "CollectorCumulativePauseTimeSec",
					"decimals": 2,
					"delta": "delta",
					"max": 10
				},
				"JavaHeapUse": {
					"title": "Zing Heap Usage (Mb)",
					"mbean": "com.azul.zing:type=Memory",
					"attribute": "JavaHeapUse",
					"divideBy": 1048576,
					"decimals": 0
				},
				"ObjectAllocation": {
					"title": "Zing Object Allocation Count",
					"mbean": "com.azul.zing:type=Memory",
					"attribute": "ObjectsWithFinalizerCumulativeAllocationCount",
					"delta": "delta"
				}
			},
			"scmVersion": "os"
		},
		
